<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:local="clr-namespace:SPRINTSUI.Themes">
    <Color x:Key="App_White_color">#000000</Color>
    <Color x:Key="App_Black_color">#FFFFFF</Color>
    <Color x:Key="App_BlueDark">#3FA6FF</Color>

    <Style x:Key="AppTextStyle" TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="{DynamicResource App_BlueDark}" />
    </Style>

</ResourceDictionary>